@article{garnelo2018neural,
  title   = {Neural processes},
  author  = {Garnelo, Marta and Schwarz, Jonathan and Rosenbaum, Dan and Viola, Fabio and Rezende, Danilo J and Eslami, SM and Teh, Yee Whye},
  journal = {arXiv preprint arXiv:1807.01622},
  year    = {2018},
  annote  = {Neural Processes (NPs) combine neural networks and GPs. The intent is to create a neural network that emulates a stochastic process: it will learn to model distributions over functions, estimate uncertainty over predictions conditioned on observations, and require fewer training resources. It is additionally linear in the number of observation and query points, rather than cubed (as in GPs). Necessary and sufficient conditions to define a stochastic process are exchangeability and consistency. In a neural process, we assume that a stoachastic process $F$ can be parameterised by a high-dimensional random vector $z$ such that $F(x) = g(x,z)$ where $g$ is fixed and learnable. Then the generative model is defined not unlike a variational autoencoder: $p(z)$ is a multivariate standard normal and the function $g$ is a NN which captures model complexities. To train, multiple datasets are required concurrently so to learn the variability of random functions. $g$ can be learned using amortized variational inference, in which the ELBO to optimize is with respect to the conditional distributions of context given targets. The NP ultimately consists of an encoder which takes input pairs and produces a representation. An aggregator summarises these encoded inputs; the mean function is used as it is order invariant. A conditional decoder then takes sampled global latent variables and new targets and outputs predictions for the targets.}
}

